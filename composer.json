{
	"name": "plugin/owc-mijn-services",
	"description": "Retrieve information from ZGW to show in Mijn Services",
	"version": "0.6.0",
	"type": "wordpress-plugin",
	"scripts": {
		"phpcs": "./vendor/squizlabs/php_codesniffer/bin/phpcs",
		"phpcbf": "phpcbf",
		"post-install-cmd": [
			"composer dump-autoload"
		],
		"post-update-cmd": [
			"composer dump-autoload"
		]
	},
	"autoload": {
		"psr-4": {
			"OWC\\My_Services\\": "src/"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"composer/installers": true
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		},
		{
			"type": "vcs",
			"url": "git@github.com:OpenWebconcept/owc-zgw-api.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:OpenWebconcept/idp-userdata.git"
		}
	],
	"require": {
		"php": "^8.1",
		"php-di/php-di": "^7.0",
		"owc/zgw-api": "^2.4",
		"owc/idp-userdata": "^1.1",
		"eftec/bladeone": "^4.19"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.7",
		"wp-coding-standards/wpcs": "^3.0"
	}
}
